{
  "extends": ["eslint:recommended", "next", "next/core-web-vitals"],
  "rules": {
    "eqeqeq": "error",
    "no-console": "warn"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "ignorePatterns": ["node_modules", "build", "dist", "public"]
}
